#!/usr/bin/env php
<?php


foreach ([__DIR__ . '/../../../autoload.php', __DIR__ . '/../vendor/autoload.php'] as $file) {
    if (file_exists($file)) {
        require $file;
        $loaded = true;
        break;
    }
}

use Symfony\Component\Console\Application;

$application = new Application();


$application->add(new \Jackal\Youtubbalo\Command\CredentialCommand());

$application->run();